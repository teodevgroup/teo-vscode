{
  "name": "teo-vscode",
  "displayName": "Teo",
  "description": "Teo language support",
  "version": "0.2.12",
  "publisher": "yeannylam",
  "repository": {
    "type": "git",
    "url": "https://github.com/victorteokw/teo-vscode.git"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "scripts": {
    "prebuild": "ts-node buildSyntax.ts",
    "build": "tsc"
  },
  "main": "./dist/src/extension.js",
  "icon": "images/icon.png",
  "activationEvents": [
    "onLanguage:teo"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "teo",
        "aliases": [
          "Teo",
          "teo",
          "TEO"
        ],
        "extensions": [
          ".teo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "teo",
        "scopeName": "source.teo",
        "path": "./syntaxes/teo.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "@teocloud/teo-language-server": "../teo-language-server",
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/vscode": "^1.82.0",
    "@vscode/vsce": "^2.21.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "yaml": "^2.3.4"
  }
}
